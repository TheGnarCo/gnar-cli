{
  "name": "gnar-cli",
  "version": "0.0.6",
  "description": "Add Gnar approved configurations to your apps in a jiffy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/TheGnarCo/gnar-node-cli",
  "author": "Blake Williams",
  "license": "MIT",
  "scripts": {
    "prepare": "tsc",
    "precommit": "lint-staged",
    "start": "tsc --watch",
    "lint": "yarn run tslint -c tslint.json '**/*.ts' '**/*.tsx'",
    "prettify": "prettier '*.{js,ts,tsx}' '{src,app,__tests__}/**/*.{js,ts,tsx}' --write"
  },
  "bin": {
    "gnar": "dist/index.js"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/lodash": "^4.14.102",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "@types/node": "^9.4.0",
    "commander": "^2.13.0",
    "lodash": "^4.17.5"
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}